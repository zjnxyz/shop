<% if !target.blank? %>
  <td><%= target.number %></td>
  <td>
  	<div class="tile wide imagetext wideimage">
        <%= image_tag(img_url(target.img_url_url(:thumb_medium))) %>
        <div class="textover-wrapper">
          <div class="text2"><%=target.name %></div>
        </div>
    </div>
  </td>
<% else %>
<td colspan="2">该商品已被删除</td> 
<% end %>