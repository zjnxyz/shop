<div class="form-horizontal">
  <%= form_for(@member,:html=>{:id=>"form_validator"}) do |f| %>
    
    <%= render "share/error_message", :target => @member %>


      <div class="control-group">
        <label class="control-label" for="inputEmail">会员类型名：</label>
        <div class="controls">
          <%= f.text_field :name,:placeholder=>"会员名",:calss=>"validate[required]" %>
        </div>
      </div>

      <div class="control-group">
        <label class="control-label" for="inputEmail">会员折扣：</label>
        <div class="controls">
          <%= f.text_field :discount,:placeholder=>"折扣",:calss=>"validate[required]" %>
        </div>
      </div>

      <div class="control-group">
        <label class="control-label" for="inputEmail">积分转换率：</label>
        <div class="controls">
          <%= f.text_field :conversion_rate,:placeholder=>"积分转换率",:calss=>"validate[required]" %>
        </div>
      </div>

      <div class="control-group">
        <div class="controls">
          <%= f.submit "保存",:class=>"btn btn-primary" %>
        </div>
      </div>

  <% end %>

</div>


