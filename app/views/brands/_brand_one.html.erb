<td><%=target.id %></td>
<td>
  <div class="thumbnail">
    <%= image_tag(target.imgurl_url(:thumb_large)) %>
    <div class="caption">
      <h3><%= target.name %></h3>
    </div>
  </div>
</td>
<td><%= format_date(target.created_at) %></td>
<td align="center">
  <br><br><br><br>
  <%= link_to edit_brand_path(target),:class => "btn btn-primary" do %> 
     <i class="icon-edit"> </i> 修改
   <% end %>
   <% if false %>
      <br><br>
    <%= link_to target, method: :delete,:class=>"btn btn-inverse", data: { confirm: '你确定要删除吗?' } do %>
     <i class="icon-trash"> </i> 删除
    <% end %>
   <% end %>

</td>