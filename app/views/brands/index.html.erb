<div class="page-header">
  <h3>品牌清单</h3>

</div>
<%= render "share/notice_message", :target => notice %>


<div>
  <div align="right">
    <%= link_to new_brand_path, :class=>"btn btn-success btn-large" do %>
     <i class="icon-plus"></i>添加
    <% end %>
  </div>
  <table class="table table-bordered">
    <thead>
    <tr>
      <th>品牌编码</th>
      <th>品牌名</th>
      <th>创建日期</th>
      <th>操作</th>
    </tr>
    </thead>
    <tbody>
      <%= render @brands,:locals => {:i=>0} %>
    </tbody>
  </table>
</div>



